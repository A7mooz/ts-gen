{
    "name": "@a7mooz/ts-gen",
    "version": "1.3.0",
    "description": "Typescript/JavaScript project generator",
    "scripts": {
        "build": "tsup",
        "build-fast": "tsup --no-dts",
        "lint": "eslint src/**/*.ts --fix",
        "pretty": "prettier -w .",
        "fmt": "pnpm pretty && pnpm lint",
        "dev": "pnpm build-fast --silent && ./dist/bin.mjs",
        "dev:win": "pnpm build-fast --silent && node --enable-source-maps dist/bin.mjs",
        "prepare": "husky install"
    },
    "bin": {
        "ts-gen": "dist/bin.mjs"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
    },
    "directories": {
        "lib": "src"
    },
    "files": [
        "dist",
        "template/**/*"
    ],
    "keywords": [
        "typescript",
        "javascript",
        "ts",
        "js",
        "cli"
    ],
    "author": "A7mooz",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/A7mooz/ts-gen.git"
    },
    "bugs": {
        "url": "https://github.com/A7mooz/ts-gen/issues"
    },
    "homepage": "https://github.com/A7mooz/ts-gen#readme",
    "dependencies": {
        "@clack/prompts": "^0.7.0",
        "colorette": "^2.0.20",
        "execa": "^8.0.1",
        "gradient-string": "^2.0.2",
        "merge": "^2.1.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.8.0",
        "@commitlint/config-conventional": "^17.8.0",
        "@types/gradient-string": "^1.1.3",
        "@types/node": "^18.18.5",
        "@typescript-eslint/eslint-plugin": "^6.7.5",
        "@typescript-eslint/parser": "^6.7.5",
        "eslint": "^8.51.0",
        "eslint-config-prettier": "^9.0.0",
        "husky": "^8.0.3",
        "lint-staged": "^14.0.1",
        "prettier": "^3.0.3",
        "tsup": "^7.2.0",
        "typescript": "^5.2.2"
    },
    "engines": {
        "node": ">=16.11.0"
    },
    "packageManager": "pnpm@8.9.2"
}
